<% for(var i=0;i<users.length;i++) {%>
<tr>
    <td><%= i + 1 %></td>
    <td><%= users[i].nome %></td>
    <td><%= users[i].descricao %> </td>
    <td><%= users[i].quantidade %></td>
    <td>
        <a href="/update-caixa?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id="<%= users[i]._id%>" >
            <span class="text-gradient"><i class="fas fa-trash"></i></span>
        </a>
    </td>
</tr>
<% } %>